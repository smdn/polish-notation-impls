{
  "Name": "Test cases of number formats (C-printf equivalent)",
  "TargetImplementations": [ "c", "cpp", "csharp", "powershell", "python", "visualbasic" ],
  "TestCases": [
    // test cases for floating point formatting equivalent to '%.17g' of C printf
    { "Input": "1/3",   "ExpectedCalculationResult": "0.33333333333333331" },
    { "Input": "2/3",   "ExpectedCalculationResult": "0.66666666666666663" },
    { "Input": "1/7",   "ExpectedCalculationResult": "0.14285714285714285" },
    { "Input": "10000000000000000/1",           "ExpectedCalculationResult": "10000000000000000" },
    { "Input": "1/10000000000000000",           "ExpectedCalculationResult": "9.9999999999999998e-17" },
    { "Input": "99999999999999999/1",           "ExpectedCalculationResult": "1e+17" },
    { "Input": "(3/2)*(10000000000000000/1)",   "ExpectedCalculationResult": "15000000000000000" },
    { "Input": "(3/2)/(10000000000000000/1)",   "ExpectedCalculationResult": "1.5e-16" },

    // test cases for floating point formatting against the expression of left and right node
    // tests that no formatting is performed on expressions in non-calculated nodes
    { "Input": "L+(1/7)", "ExpectedExitCode": 2, "ExpectAsCalculatedExpression": "(L + 0.14285714285714285)" },
    { "Input": "(1/7)+R", "ExpectedExitCode": 2, "ExpectAsCalculatedExpression": "(0.14285714285714285 + R)" },
    { "Input": "L+0.142857142857142857142857142857", "ExpectedExitCode": 2, "ExpectAsCalculatedExpression": "(L + 0.142857142857142857142857142857)" },
    { "Input": "0.142857142857142857142857142857+R", "ExpectedExitCode": 2, "ExpectAsCalculatedExpression": "(0.142857142857142857142857142857 + R)" },
  ]
}
