{
  "Name": "Test cases of calculable expressions",
  "TestCases": [
    { "Input": "1",                 "ExpectedCalculationResult": "1" },
    { "Input": "(1)",               "ExpectedCalculationResult": "1" },
    { "Input": "((1))",             "ExpectedCalculationResult": "1" },
    { "Input": "1+2",               "ExpectedCalculationResult": "3" },
    { "Input": "(1+2)",             "ExpectedCalculationResult": "3" },
    { "Input": "((1+2))",           "ExpectedCalculationResult": "3" },
    { "Input": "1+(2)",             "ExpectedCalculationResult": "3" },
    { "Input": "(1)+2",             "ExpectedCalculationResult": "3" },
    { "Input": "(1)+(2)",           "ExpectedCalculationResult": "3" },
    { "Input": "((1)+(2))",         "ExpectedCalculationResult": "3" },
    { "Input": "(((1)+(2)))",       "ExpectedCalculationResult": "3" },
    { "Input": "((((1))+((2))))",   "ExpectedCalculationResult": "3" },
    { "Input": "10-3+2",            "ExpectedCalculationResult": "9" }, // is treated as expression "(10-3)+2"
    { "Input": "(10-3)+2",          "ExpectedCalculationResult": "9" },
    { "Input": "10+3-2",            "ExpectedCalculationResult": "11" }, // is treated as expression "(10+3)-2"
    { "Input": "(10-3)+2",          "ExpectedCalculationResult": "9" },
    { "Input": "1-3+2",             "ExpectedCalculationResult": "0" }, // is treated as expression "(1-3)+2"
    { "Input": "1-3+2-4",           "ExpectedCalculationResult": "-4" }, // is treated as expression "((1-3)+2)-4"
    { "Input": "2+5*3-4",           "ExpectedCalculationResult": "13" },
    { "Input": "(2+5)*3-4",         "ExpectedCalculationResult": "17" },
    { "Input": "2+5*(3-4)",         "ExpectedCalculationResult": "-3" },
    { "Input": "2+(5*3-4)",         "ExpectedCalculationResult": "13" },
    { "Input": "(2+5*3)-4",         "ExpectedCalculationResult": "13" },
    { "Input": "(2+5)*(3-4)",       "ExpectedCalculationResult": "-7" },
    { "Input": "((2+(5*3))-4)",     "ExpectedCalculationResult": "13" },
    { "Input": "1+4*2+(7-3)/2",     "ExpectedCalculationResult": "11" },
    { "Input": "1+4*(2+(7-3))/2",   "ExpectedCalculationResult": "13" },

    /*
     * numeric formatting
     */
    { "Input": "1*1.5",             "ExpectedCalculationResult": "1.5" },
    { "Input": "3/2",               "ExpectedCalculationResult": "1.5" },
    { "Input": "1/2",               "ExpectedCalculationResult": "0.5" },
    { "Input": "1/4",               "ExpectedCalculationResult": "0.25" },
    { "Input": "1/8",               "ExpectedCalculationResult": "0.125" },
    { "Input": "1/3",               "ExpectedCalculationResult": "0.333333333333333" },
    { "Input": "2/3",               "ExpectedCalculationResult": "0.666666666666667" },
    { "Input": "10000000000000000/1",           "ExpectedCalculationResult": "1e+16" },
    { "Input": "1/10000000000000000",           "ExpectedCalculationResult": "1e-16" },
    { "Input": "(3/2)*(10000000000000000/1)",   "ExpectedCalculationResult": "1.5e+16" },

    /*
     * expression which contains space
     */
    { "Input": " 1+2",        "ExpectedCalculationResult": "3" },
    { "Input": "1+2 ",        "ExpectedCalculationResult": "3" },
    { "Input": " 1+2 ",       "ExpectedCalculationResult": "3" },
    { "Input": "1 +2",        "ExpectedCalculationResult": "3" },
    { "Input": "1+ 2",        "ExpectedCalculationResult": "3" },
    { "Input": "1 + 2",       "ExpectedCalculationResult": "3" },
    { "Input": " 1 + 2 ",     "ExpectedCalculationResult": "3" },
    { "Input": " ( 1 + 2) ",  "ExpectedCalculationResult": "3" },

    /*
     * implicit multiplication
     */
    { "Input": "2(1+2)",      "ExpectedCalculationResult": "2(1+2)" },
  ]
}
