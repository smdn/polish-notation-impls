{
  "Name": "Test cases of number formats (specific for Intl.NumberFormat and java.text.NumberFormat)",
  "TargetImplementations": [ "java", "javascript" ],
  "TestCases": [
    // test cases for floating point formatting specific to Intl.NumberFormat and/or java.text.NumberFormat (maximumSignificantDigits = 17)
    { "Input": "1/3",   "ExpectedCalculationResult": "0.3333333333333333" },
    { "Input": "2/3",   "ExpectedCalculationResult": "0.6666666666666666" },
    { "Input": "1/7",   "ExpectedCalculationResult": "0.14285714285714285" },
    { "Input": "10000000000000000/1",           "ExpectedCalculationResult": "10000000000000000" },
    { "Input": "1/10000000000000000",           "ExpectedCalculationResult": "0.0000000000000001" },
    { "Input": "99999999999999999/1",           "ExpectedCalculationResult": "100000000000000000" },
    { "Input": "(3/2)*(10000000000000000/1)",   "ExpectedCalculationResult": "15000000000000000" },
    { "Input": "(3/2)/(10000000000000000/1)",   "ExpectedCalculationResult": "0.00000000000000015" },
  ]
}
